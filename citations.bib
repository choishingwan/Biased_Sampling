	abstract = {Population structure, including population stratification and cryptic relatedness, can cause spurious associations in genome-wide association studies (GWAS). Usually, the scaled median or mean test statistic for association calculated from multiple single-nucleotide-polymorphisms across the genome is used to assess such effects, and ‘genomic control’ can be applied subsequently to adjust test statistics at individual loci by a genomic inflation factor. Published GWAS have clearly shown that there are many loci underlying genetic variation for a wide range of complex diseases and traits, implying that a substantial proportion of the genome should show inflation of the test statistic. Here, we show by theory, simulation and analysis of data that in the absence of population structure and other technical artefacts, but in the presence of polygenic inheritance, substantial genomic inflation is expected. Its magnitude depends on sample size, heritability, linkage disequilibrium structure and the number of causal variants. Our predictions are consistent with empirical observations on height in independent samples of {\textasciitilde}4000 and {\textasciitilde}133 000 individuals.},
	language = {en},
	number = {7},
	urldate = {2017-04-27},
	journal = {European Journal of Human Genetics},
	author = {Yang, Jian and Weedon, Michael N. and Purcell, Shaun and Lettre, Guillaume and Estrada, Karol and Willer, Cristen J. and Smith, Albert V. and Ingelsson, Erik and O'Connell, Jeffrey R. and Mangino, Massimo and Mägi, Reedik and Madden, Pamela A. and Heath, Andrew C. and Nyholt, Dale R. and Martin, Nicholas G. and Montgomery, Grant W. and Frayling, Timothy M. and Hirschhorn, Joel N. and McCarthy, Mark I. and Goddard, Michael E. and Visscher, Peter M.},
	month = jul,
	year = {2011},
	keywords = {Genome-Wide Association Study, genomic inflation factor, polygenic inheritance},
	pages = {807--812},
	file = {Full Text PDF:C\:\\Users\\shingwan\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\e3qwvvhl.default\\zotero\\storage\\E42KK22R\\Yang et al. - 2011 - Genomic inflation factors under polygenic inherita.pdf:application/pdf;Snapshot:C\:\\Users\\shingwan\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\e3qwvvhl.default\\zotero\\storage\\GVX8MMG4\\ejhg201139a.html:text/html}
