@article{agerbo_polygenic_2015,
	title = {Polygenic {Risk} {Score}, {Parental} {Socioeconomic} {Status}, {Family} {History} of {Psychiatric} {Disorders}, and the {Risk} for {Schizophrenia}: {A} {Danish} {Population}-{Based} {Study} and {Meta}-analysis},
	volume = {72},
	issn = {2168-622X},
	shorttitle = {Polygenic {Risk} {Score}, {Parental} {Socioeconomic} {Status}, {Family} {History} of {Psychiatric} {Disorders}, and the {Risk} for {Schizophrenia}},
	url = {http://jamanetwork.com/journals/jamapsychiatry/fullarticle/2211892},
	doi = {10.1001/jamapsychiatry.2015.0346},
	abstract = {{\textless}h3{\textgreater}Importance{\textless}/h3{\textgreater}{\textless}p{\textgreater}Schizophrenia has a complex etiology influenced both by genetic and nongenetic factors but disentangling these factors is difficult.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Objective{\textless}/h3{\textgreater}{\textless}p{\textgreater}To estimate (1) how strongly the risk for schizophrenia relates to the mutual effect of the polygenic risk score, parental socioeconomic status, and family history of psychiatric disorders; (2) the fraction of cases that could be prevented if no one was exposed to these factors; (3) whether family background interacts with an individual’s genetic liability so that specific subgroups are particularly risk prone; and (4) to what extent a proband’s genetic makeup mediates the risk associated with familial background.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Design, Settings, and Participants{\textless}/h3{\textgreater}{\textless}p{\textgreater}We conducted a nested case-control study based on Danish population-based registers. The study consisted of 866 patients diagnosed as having schizophrenia between January 1, 1994, and December 31, 2006, and 871 matched control individuals. Genome-wide data and family psychiatric and socioeconomic background information were obtained from neonatal biobanks and national registers. Results from a separate meta-analysis (34 600 cases and 45 968 control individuals) were applied to calculate polygenic risk scores.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Exposures{\textless}/h3{\textgreater}{\textless}p{\textgreater}Polygenic risk scores, parental socioeconomic status, and family psychiatric history.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Main Outcomes and Measures{\textless}/h3{\textgreater}{\textless}p{\textgreater}Odds ratios (ORs), attributable risks, liability\textit{R}$^{\textrm{2 }}$values, and proportions mediated.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Results{\textless}/h3{\textgreater}{\textless}p{\textgreater}Schizophrenia was associated with the polygenic risk score (OR, 8.01; 95\% CI, 4.53-14.16 for highest vs lowest decile), socioeconomic status (OR, 8.10; 95\% CI, 3.24-20.3 for 6 vs no exposures), and a history of schizophrenia/psychoses (OR, 4.18; 95\% CI, 2.57-6.79). The\textit{R}$^{\textrm{2 }}$values were 3.4\% (95\% CI, 2.1-4.6) for the polygenic risk score, 3.1\% (95\% CI, 1.9-4.3) for parental socioeconomic status, and 3.4\% (95\% CI, 2.1-4.6) for family history. Socioeconomic status and psychiatric history accounted for 45.8\% (95\% CI, 36.1-55.5) and 25.8\% (95\% CI, 21.2-30.5) of cases, respectively. There was an interaction between the polygenic risk score and family history (\textit{P} = .03). A total of 17.4\% (95\% CI, 9.1-26.6) of the effect associated with family history of schizophrenia/psychoses was mediated through the polygenic risk score.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Conclusions and Relevance{\textless}/h3{\textgreater}{\textless}p{\textgreater}Schizophrenia was associated with the polygenic risk score, family psychiatric history, and socioeconomic status. Our study demonstrated that family history of schizophrenia/psychoses is partly mediated through the individual’s genetic liability.{\textless}/p{\textgreater}},
	number = {7},
	urldate = {2017-04-27},
	journal = {JAMA Psychiatry},
	author = {Agerbo, Esben and Sullivan, Patrick F. and Vilhjálmsson, Bjarni J. and Pedersen, Carsten B. and Mors, Ole and Børglum, Anders D. and Hougaard, David M. and Hollegaard, Mads V. and Meier, Sandra and Mattheisen, Manuel and Ripke, Stephan and Wray, Naomi R. and Mortensen, Preben B.},
	month = jul,
	year = {2015},
	pages = {635--641},
	file = {Full Text PDF:C\:\\Users\\shingwan\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\e3qwvvhl.default\\zotero\\storage\\8EPWGXKH\\Agerbo et al. - 2015 - Polygenic Risk Score, Parental Socioeconomic Statu.pdf:application/pdf;Full Text PDF:C\:\\Users\\shingwan\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\e3qwvvhl.default\\zotero\\storage\\SW9CASQ8\\Agerbo et al. - 2015 - Polygenic Risk Score, Parental Socioeconomic Statu.pdf:application/pdf;Snapshot:C\:\\Users\\shingwan\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\e3qwvvhl.default\\zotero\\storage\\BST6JHDG\\2211892.html:text/html;Snapshot:C\:\\Users\\shingwan\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\e3qwvvhl.default\\zotero\\storage\\FUSZ2PP8\\2211892.html:text/html}
