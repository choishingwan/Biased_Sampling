	url = {https://www.nature.com/ng/journal/v48/n9/full/ng.3623.html},
	doi = {10.1038/ng.3623},
	abstract = {Despite strong evidence supporting the heritability of major depressive disorder (MDD), previous genome-wide studies were unable to identify risk loci among individuals of European descent. We used self-report data from 75,607 individuals reporting clinical diagnosis of depression and 231,747 individuals reporting no history of depression through 23andMe and carried out meta-analysis of these results with published MDD genome-wide association study results. We identified five independent variants from four regions associated with self-report of clinical diagnosis or treatment for depression. Loci with a P value {\textless}1.0 × 10−5 in the meta-analysis were further analyzed in a replication data set (45,773 cases and 106,354 controls) from 23andMe. A total of 17 independent SNPs from 15 regions reached genome-wide significance after joint analysis over all three data sets. Some of these loci were also implicated in genome-wide association studies of related psychiatric traits. These studies provide evidence for large-scale consumer genomic data as a powerful and efficient complement to data collected from traditional means of ascertainment for neuropsychiatric disease genomics.},
	language = {en},
	number = {9},
	urldate = {2017-04-27},
	journal = {Nature Genetics},
	author = {Hyde, Craig L. and Nagle, Michael W. and Tian, Chao and Chen, Xing and Paciga, Sara A. and Wendland, Jens R. and Tung, Joyce Y. and Hinds, David A. and Perlis, Roy H. and Winslow, Ashley R.},
	month = sep,
	year = {2016},
	keywords = {depression, Genome-wide association studies},
	pages = {1031--1036},
	file = {Full Text PDF:C\:\\Users\\shingwan\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\e3qwvvhl.default\\zotero\\storage\\3HSIUTUW\\Hyde et al. - 2016 - Identification of 15 genetic loci associated with .pdf:application/pdf;Snapshot:C\:\\Users\\shingwan\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\e3qwvvhl.default\\zotero\\storage\\2UWZRS6V\\ng.3623.html:text/html}
